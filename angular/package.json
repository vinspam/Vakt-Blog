{
  "name": "varkt",
  "version": "0.0.1",
  "scripts": {
    "start": "ng serve --open",
    "start:zh": "ng serve --configuration=zh --open --port 4201",
    "build": "ng build --prod --localize",
    "build:zh": "ng build --configuration=production-zh-Hans --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --output-path=src/locale",
    "dev:ssr": "ng run varkt:serve-ssr",
    "serve:ssr": "node dist/varkt/server/main.js",
    "build:ssr": "ng build --prod && ng run varkt:server:production",
    "prerender": "ng run varkt:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/localize": "^11.0.0",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/platform-server": "^11.0.0",
    "@angular/pwa": "^0.1001.7",
    "@angular/router": "^11.0.0",
    "@angular/service-worker": "^11.0.0",
    "@nguniversal/express-engine": "^11.0.0",
    "express": "^4.18.2",
    "ngx-markdown": "^10.1.1",
    "rxjs": "^6.6.3",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.1",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@nguniversal/builders": "^11.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.7",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "prettier": "2.2.1",
    "protractor": "^7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  }
}
