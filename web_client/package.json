{
  "name": "holacode",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --open",
    "start:zh": "ng serve --configuration=zh --open --port 4201",
    "build": "ng build --prod && npm run build:zh",
    "build:zh": "ng build --configuration=production-zh --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --output-path=src/locale",
    "dev:ssr": "ng run holacode:serve-ssr",
    "serve:ssr": "node dist/holacode/server/main.js",
    "build:ssr": "ng build --prod && ng run holacode:server:production",
    "prerender": "ng run holacode:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/localize": "^9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/platform-server": "^9.1.0",
    "@angular/pwa": "^0.901.0",
    "@angular/router": "^9.1.0",
    "@angular/service-worker": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
    "express": "^4.15.2",
    "ngx-markdown": "^9.0.0",
    "quill": "^1.3.7",
    "rxjs": "^6.5.5",
    "ts-md5": "^1.2.7",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@nguniversal/builders": "^9.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.11.0",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  }
}
